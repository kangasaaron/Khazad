                           "_width": "25%"
                        },
                        {
                           "_height": "100%",
                           "_id": "SeedTextField",
                           "_name": "textfield",
                           "_width": "285px",
                           "_maxLength": "50"
                        }
                     ],
                     "_childLayout": "horizontal",
                     "_height": "40px",
                     "_padding": "5px",
                     "_width": "100%"
                  },
                  {
                     "control": [
                        {
                           "_font": "aurulent-sans-16.fnt",
                           "_height": "50px",
                           "_id": "label5",
                           "_name": "label",
                           "_text": "Size X",
                           "_width": "50%",
                           "_x": "0",
                           "_y": "5"
                        },
                        {
                           "_font": "aurulent-sans-16.fnt",
                           "_height": "50px",
                           "_id": "label3",
                           "_name": "label",
                           "_text": "Size Y",
                           "_width": "50%",
                           "_x": "253",
                           "_y": "2"
                        }
                     ],
                     "_childLayout": "horizontal",
                     "_height": "80px",
                     "_id": "panel11",
                     "_width": "100%"
                  },
                  {
                     "panel": [
                        {
                           "control": [
                              {
                                 "interact": {
                                    "_onClick": "CancelSetup()"
                                 },
                                 "_childLayout": "center",
                                 "_height": "38px",
                                 "_id": "CancelButton",
                                 "_label": "Cancel",
                                 "_name": "button",
                                 "_width": "130px"
                              }
                           ],
                           "_childLayout": "center",
                           "_height": "89px",
                           "_id": "panel0",
